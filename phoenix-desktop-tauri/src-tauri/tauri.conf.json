{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Sola AGI",
  "version": "1.0.0",
  "identifier": "com.sola.agi",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../frontend_desktop/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "Sola AGI",
        "width": 1100,
        "height": 720,
        "resizable": true,
        "minWidth": 800,
        "minHeight": 600,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "category": "Productivity"
  }
}

